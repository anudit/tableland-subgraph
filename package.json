{
  "name": "tableland",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "auth": "graph auth --studio",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ anudit/tableland",
    "create-local": "graph create --node http://localhost:8020/ anudit/tableland",
    "remove-local": "graph remove --node http://localhost:8020/ anudit/tableland",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 anudit/tableland",
    "prepare:poly": "mustache config/poly.json subgraph.template.yaml > subgraph.yaml",
    "prepare:mumbai": "mustache config/mumbai.json subgraph.template.yaml > subgraph.yaml",
    "prepare:op": "mustache config/op.json subgraph.template.yaml > subgraph.yaml",
    "prepare:op-goerli": "mustache config/op-goerli.json subgraph.template.yaml > subgraph.yaml",
    "prepare:arb": "mustache config/arb.json subgraph.template.yaml > subgraph.yaml",
    "prepare:arb-goerli": "mustache config/arb-goerli.json subgraph.template.yaml > subgraph.yaml",
    "prepare:arb-nova": "mustache config/arb-nova.json subgraph.template.yaml > subgraph.yaml",
    "prepare:eth": "mustache config/eth.json subgraph.template.yaml > subgraph.yaml",
    "prepare:goerli": "mustache config/goerli.json subgraph.template.yaml > subgraph.yaml",
    "prepare:sepolia": "mustache config/sepolia.json subgraph.template.yaml > subgraph.yaml",
    "deploy:poly": "pnpm prepare:poly && graph deploy --node https://api.thegraph.com/deploy/ anudit/tableland-polygon",
    "deploy:mumbai": "pnpm prepare:mumbai && graph deploy --studio tablescan-polygon-mumbai",
    "deploy:op": "pnpm prepare:op && graph deploy --node https://api.thegraph.com/deploy/ anudit/tableland-optimism",
    "deploy:op-goerli": "pnpm prepare:op-goerli && graph deploy --studio tablescan-optimism-goerli",
    "deploy:arb": "pnpm prepare:arb && graph deploy --studio tablescan-arbitrum",
    "deploy:arb-goerli": "pnpm prepare:arb-goerli && graph deploy --studio tablescan-arbitrum-goerli",
    "deploy:arb-nova": "pnpm prepare:arb-nova && graph deploy --product hosted-service anudit/tablescan-arbitrum-nova",
    "deploy:eth": "pnpm prepare:eth && graph deploy --studio tableland-ethereum",
    "deploy:goerli": "pnpm prepare:goerli && graph deploy --studio tableland-ethereum-goerli",
    "deploy:sepolia": "pnpm prepare:sepolia && graph deploy --studio tablescan-ethereum-sepolia",
    "deploy:all": "pnpm deploy:poly && pnpm deploy:mumbai && pnpm deploy:op && pnpm deploy:op-goerli && pnpm deploy:arb && pnpm deploy:arb-goerli && pnpm deploy:eth && pnpm deploy:goerli"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.48.0",
    "@graphprotocol/graph-ts": "0.29.3",
    "mustache": "^4.2.0"
  }
}
